[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://tabletop-rust/target/debug/libtabletop_rust.so"
linux.release.x86_64 =   "res://tabletop-rust/target/release/libtabletop_rust.so"
windows.debug.x86_64 =   "res://tabletop-rust/target/debug/tabletop_rust.dll" 
windows.release.x86_64 = "res://tabletop-rust/target/release/tabletop_rust.dll"
macos.debug =            "res://tabletop-rust/target/debug/libtabletop_rust.dylib"
macos.release =          "res://tabletop-rust/target/release/libtabletop_rust.dylib"
macos.debug.arm64 =      "res://tabletop-rust/target/debug/libtabletop_rust.dylib"
macos.release.arm64 =    "res://tabletop-rust/target/release/libtabletop_rust.dylib"

